cages <- cages %>%
dplyr::select(Animal_ID, sex, DateTime, date, time, weight, Activity, EE, H2Omg,
H2Oml, Deg_C, CO2, VO2, VCO2, RQ, SD_VO2, SD_VCO2, SD_H2Oml,
